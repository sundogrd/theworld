{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "lint-staged": "lint-staged",
    "style-lint": "stylelint \"./**/*.less\" --config stylelint.config.js  --syntax less",
    "style-lint-fix": "stylelint \"./**/*.less\" --config stylelint.config.js  --syntax less --fix",
    "dev": "concurrently -k \"yarn run server\" \"wait-on http://localhost:3000 && yarn run dev-start\"",
    "webpack-dev": "webpack --mode development --config ./webpack.config.dev.js",
    "build": "webpack --mode production --config ./webpack.config.prod.js",
    "server": "yarn run webpack-dev && webpack-dev-server --config ./webpack.config.server.js",
    "dev-start": "cross-env ELECTRON_DEV=dev electron ./dist/main.js"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "electron": "^6.0.7",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.5",
    "mobx-react-devtools": "^6.1.1",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.10.0",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-router-dom": "^5.1.2",
    "react-use": "^12.2.3"
  }
}
